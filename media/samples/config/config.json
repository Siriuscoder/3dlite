{
    // common settings
    "LogLevel": 2,
    "LogFlushAlways": false,
    "logMuteStd": false,
    "TextureSettings": {
        "Anisotropy": 8,
        "Compression": true
    },
    // Video settings
    // Window size, caption, color settings, etc
    "VideoSettings": {
        "GLProfile": "core",
        "GLVersionMajor": 3,
        "GLVersionMinor": 3,
        "Width": 2048,
        "Height": 1080,
        "Caption": "Sample scene",
        "ColorBits": 24,
        "Fullscreen": false,
        "MSAA": 4,
        "VSync": false
    },
    // resource locations
    "ResourceLocations": [
        {
            "Name": "samples",
            "FileCacheMaxSize": 10240000,
            "Path": "samples/"
        }
    ],
    "FixedUpdatesInterval": 15
}